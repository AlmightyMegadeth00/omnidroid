<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Enter the values to be inserted" />
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Enter the Instance_Name" />

	<EditText android:id="@+id/dummy_iname" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Enter the Action_Data" />
	<EditText android:id="@+id/dummy_adata" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<Button android:id="@+id/dummy_store" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Save" />
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Enter the ID to be retrieved" />
	<EditText android:id="@+id/dummy_id" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />

	<Button android:id="@+id/dummy_retrieve" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Fetch that" />
	<Button android:id="@+id/dummy_getAll" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Get All Contents" />
</LinearLayout>
